{
  "name": "10th-west-moto",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cd frontend && npx vite --port 3010 --host",
    "dev:frontend": "cd frontend && npx vite --port 3010 --host",
    "dev:backend": "cd backend && node src/server.js",
    "build": "cd frontend && npx vite build",
    "install:frontend": "cd frontend && npm install --legacy-peer-deps",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "start": "cd backend && node src/server.js"
  }
}
